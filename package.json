      "cors": "^2.8.5",
      "dotenv": "^16.0.0",
      "dotenv-safe": "^8.2.0",
      "edgedb": "^1.0.2",
      "envalid": "^7.3.1",
      "express": "^4.18.1",
      "html-to-text": "^8.2.0",
